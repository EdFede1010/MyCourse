{
  "ConnectionStrings": {
    "Default": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=master;Integrated Security=True"
  },
  "Courses": {
    "PerPage": 10,
    "InHome": 3,
    "Order": {
      "By": "Rating",
      "Ascending": false,
      "Allow": [ "Title", "Rating", "CurrentPrice", "Id" ]
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    }
  },
  "ResponseCache": {
    "Home": {
      "Duration": 60,
      "Location": "Client",
      "VaryByQueryKeys": [ "page" ]
    }
  },
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 8388608
    }
  }
}